{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "./",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@views/*": ["src/views/*"]
    }
  },
  "include": ["src/**/*"]
}
